{
    "name": "example/project",
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist"
    },
    "archive": {
      "exclude": [
        "/Dossier_Technique",
        "/xdebug"
      ]
    },
    "scripts": {
      "post-install-cmd": [
        "@php artisan optimize",
        "composer dump-autoload --optimize"
      ]
    },
    "require-dev": {
        "symfony/symfony": "^7.0",
        "phpstan/phpstan": "^1.11"
    },
    "require": {
        "firebase/php-jwt": "^6.10",
        "monolog/monolog": "^2.0"
    }
}
